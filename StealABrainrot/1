local selectedBrainrot = nil
local autobuyEnabled = false
local followConnection

AutobuyTAB:DropdownAlways("  ãƒ»  Choose Speed Type", brainrots, function(value)
	selectedBrainrot = value
end, false)

local function followAndBuy()
	while autobuyEnabled and selectedBrainrot do
		local foundAnimal = nil

		for _, model in pairs(workspace.MovingAnimals:GetChildren()) do
			local info = model:FindFirstChild("HumanoidRootPart") and model.HumanoidRootPart:FindFirstChild("Info")
			local promptAttach = model.HumanoidRootPart and model.HumanoidRootPart:FindFirstChild("PromptAttachment")
			local display = info and info:FindFirstChild("AnimalOverhead")
			local nameLabel = display and display:FindFirstChild("DisplayName")
			local prompt = promptAttach and promptAttach:FindFirstChildWhichIsA("ProximityPrompt")

			if nameLabel and nameLabel.Text == selectedBrainrot and prompt then
				foundAnimal = model
				break
			end
		end

		if foundAnimal then
			local root = foundAnimal:FindFirstChild("HumanoidRootPart")
			if root then
				local distance = (RootPart.Position - root.Position).Magnitude
				if distance > 6 then
					local path = PathfindingService:CreatePath()
					path:ComputeAsync(RootPart.Position, root.Position)
					path:MoveTo(Character)
				end

				task.wait(0.5)

				local prompt = root:FindFirstChild("PromptAttachment"):FindFirstChildWhichIsA("ProximityPrompt")
				if prompt then
					fireproximityprompt(prompt)
				end
			end
		end

		task.wait(1)
	end
end

AutobuyTAB:Toggle("  ãƒ»  Buy choosed brainrot", false, function(bool)
	autobuyEnabled = bool

	if bool then
		if not followConnection then
			followConnection = RunService.Heartbeat:Connect(followAndBuy)
		end
	else
		if followConnection then
			followConnection:Disconnect()
			followConnection = nil
		end
	end
end)

AutobuyTAB:Toggle("  ãƒ»  Buy all dropdown brainrots", false, function(bool)	
	print("Disabled for bugs")
end)

